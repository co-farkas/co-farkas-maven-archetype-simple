<archetype-descriptor name="${artifactId}"
                      xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd">
  <requiredProperties>
    <requiredProperty key="version">
      <defaultValue>0.0.1-SNAPSHOT</defaultValue>
    </requiredProperty>
    <requiredProperty key="projectName"/>
    <requiredProperty key="projectDescription"/>
    <requiredProperty key="urlBase">
      <defaultValue>https://co-farkas.github.io</defaultValue>
    </requiredProperty>
    <requiredProperty key="copyrightYear">
      <defaultValue>2019</defaultValue>
    </requiredProperty>
    <requiredProperty key="copyrightOwner">
      <defaultValue>Mihaly Farkas</defaultValue>
    </requiredProperty>
    <requiredProperty key="githubAccount">
      <defaultValue>co-farkas</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true">
      <directory/>
      <includes>
        <include>.gitignore</include>
        <include>README.md</include>
        <include>LICENSE</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>.doc</directory>
    </fileSet>
    <fileSet>
      <directory>.mvn</directory>
    </fileSet>
    <fileSet>
      <directory>src/site</directory>
    </fileSet>
    <fileSet filtered="true" packaged="true">
      <directory>src/main/java</directory>
    </fileSet>
    <fileSet filtered="true" packaged="true">
      <directory>src/test/java</directory>
    </fileSet>
  </fileSets>
</archetype-descriptor>
